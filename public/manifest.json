{
  "manifest_version": 3,
  "name": "Reminder-App",
  "version": "0.8.0",
  "description": "Reminder app for all your tasks",
  "icons": {
    "128": "tasks.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Reminder App",
    "default_icon": {
      "16": "tasks.png",
      "24": "tasks.png",
      "32": "tasks.png"
    }
  },
  "permissions": ["storage"]
}
